
<% test.forEach(book =>  { %>

    <div class="row bg-light top-row">
    <div class="col-4"><img class="card-img-top resize" src="uploads/<%= book.path %>" alt="Card image"></div>
    <div class="col-8">
        <h1><%= book.title %></h1>
        <h3>Author: <%= book.authorName%> </h3>
        <h3>isbn: <%= book.isbn %></h3>
        <h2><%= book.price %></h2>
        <div></div>
        <button class="btn btn-secondary btn-sm">CONTACT</button>
    </div>
</div>

<% }); %>



